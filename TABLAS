CREATE TABLE EMPLEADO(
    ID_EMPLEADO NUMBER(10)
    NOMBRE VARCHAR(25),
    PATERNO VARCHAR(25),
    MATERNO VARCHAR(25),
    FECHA_N VARCHAR(14),
    FECHA_IN VARCHAR(14),
    ID_AREA NUMBER(4),
    ID_JEFE NUMBER(10),
    ID_SUCURSAL NUMBER(10)
);

CREATE TABLE JEFE(
    ID_JEFE NUMBER(10)
    ID_EMPLEADO NUMBER(25),
    ID_AREA NUMBER(4)
)

CREATE TABLE Area(
    ID_Area NUMBER(4),
    NOMBRE VARCHAR(25),
    ID_JEFE NUMBER(10)
)

CREATE TABLE PUESTO(
    ID_PUESTO NUMBER(4),
    NOMBRE VARCHAR(25),
    ID_AREA NUMBER(4),
    SALARIO NUMBER(6)
)

CREATE TABLE SUCURSAL(
    ID_SUCURSAL NUMBER(10)
    NOMBRE VARCHAR(25),
    ESTADO VARCHAR(25)
)

CREATE TABLE PROVEEDOR(
    ID_PROVEEDOR NUMBER(10)
    NOMBRE VARCHAR(25),
    ID_PRODUCTO VARCHAR(25),
    DIRECCION VARCHAR(25)
)

